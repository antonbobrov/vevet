{
  "name": "vevet",
  "version": "1.4.3",
  "description": "VEVET - A JavaScript library",
  "directories": {
    "dist:": "./dist"
  },
  "main": "dist/js/index.js",
  "types": "dist/types/types.d.ts",
  "sideEffects": false,
  "sass": "dist/scss/index.scss",
  "scripts": {
    "dev": "NODE_ENV=development webpack --config ./webpack/webpack.dev.conf.js",
    "build": "NODE_ENV=production webpack --config ./webpack/webpack.build.conf.js",
    "start": "webpack-dev-server --open --config ./webpack/webpack.dev.conf.js",
    "docs": "npm run docs:default && npm run docs:types",
    "docs:default": "./node_modules/.bin/jsdoc -c jsdoc/jsdoc.json -R README.md",
    "docs:types": "./node_modules/.bin/jsdoc -c jsdoc/jsdoc.types.json && node jsdoc/exportVevetJsDoc.js",
    "release": "npm publish",
    "prepare": "npm run prepare:cjs && npm run build && npm run docs",
    "prepare:cjs": "NODE_ENV=cjs babel ./src/js --out-dir ./dist/js/"
  },
  "browserslist": [
    "> 1%",
    "last 3 version"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonbobrov/vevet.git"
  },
  "author": "Anton Bobrov <anton.bobrov@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antonbobrov/vevet/issues"
  },
  "homepage": "https://github.com/antonbobrov/vevet#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@otris/jsdoc-tsd": "^1.0.4",
    "@pixi/jsdoc-template": "^2.4.2",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "braintree-jsdoc-template": "^3.3.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "escape-regex-string": "^1.0.6",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-fbjs": "^3.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jsdoc": "^10.0.0",
    "file-loader": "^4.0.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jsdoc": "^3.6.2",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "node-static": "^0.7.11",
    "normalize-scss": "^7.0.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tsd-jsdoc": "^2.3.0",
    "typescript": "^3.5.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "dom-create-element": "^1.0.2",
    "form-serialize": "^0.7.2",
    "form-to-object": "^6.0.0",
    "is-mobile": "^2.2.1",
    "isarray": "^2.0.5",
    "lerp": "^1.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "lodash.mergewith": "^4.6.2",
    "node-background-size": "^1.0.4",
    "polyfill-array-includes": "^2.0.0",
    "select-el": "^1.0.3",
    "string.prototype.includes": "^1.0.0"
  }
}
